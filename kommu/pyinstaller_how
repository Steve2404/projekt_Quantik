pyinstaller --onefile --windowed --noconfirm main.py
ldd dist/main

# Mettre à jour le système et installer les dépendances Tkinter
sudo apt update
sudo apt install python3-tk python3-venv

# Créer et activer un environnement virtuel
python3 -m venv myenv
source myenv/bin/activate

# Installer PyInstaller
pip install pyinstaller

# Créer le fichier server_gui.py (ajouter le code Tkinter ci-dessus)

# Générer l'exécutable
pyinstaller --onefile --windowed --noconfirm server_gui.py

# Vérifier les permissions
chmod +x dist/server_gui

# Exécuter l'exécutable
./dist/server_gui

